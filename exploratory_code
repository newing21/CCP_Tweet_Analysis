def ReviewsPreprocessing(text): 
    # get lowercase
    text = text.lower()
    # remove numbers    # remove numbers

    text = re.sub(r'\d+', '', text)
    # remove urls
    text = re.sub(r'^https?:\/\/.*[\r\n]*', '', text)
    # remove punctuation
    text = text.translate(text.maketrans('', '', string.punctuation))
    # strip whitespace
    text = text.strip()
    # remove stop words
    stop_words = stopwords.words('english')
    newStopWords = ['html','yet', 'â€™', '``', 'â€¦', '...', "''", 'â€˜', 'â€œ', 'â€', "'m", "'re", "'s", "'ve", 'amp', 'https', "n't", 'rt', 
                   'aâ€¦', 'co', 'iâ€¦','itÃ¢â‚¬â„¢s', 'Ã¢â‚¬\x9d', 'one', 'could','would', 'also', 'Ã¢â‚¬', 'said', 'â€”', 'itÃ¢', 'new',
                  'get', 'say', 'way', 'day', 'ca', 'many', 'get', 'like', 'end', 'gtv', 'go', 'https']
    stop_words.extend(newStopWords)
    stop_words = set(stop_words)
    tokens = word_tokenize(text)
    words = [w for w in tokens if not w in stop_words]
    text = " ".join(w for w in words)
    text = text.replace('thisway', 'this way')
    text = text.replace('figures', 'figure')
    text = text.replace('hong kong', 'hongkong')

    return text
posts = df_en.tweet_text.values
